<!doctype html>
<html lang="#locale()">
<head> #import("title") <meta http-equiv="content-type" content="text/html; charset=us-ascii"></head>
<body style="overflow-wrap: break-word; -webkit-nbsp-mode: space; line-break: after-white-space;">
    <table style="width: 600px" border="0" cellspacing="0" cellpadding="0" align="center" bgcolor="#ffffff" class="MainContainer" style="font-variant-caps: normal; color: rgb(0, 0, 0); font-family: Helvetica, Arial, serif; font-size: 12px;">
    <tbody>
        <tr><td>
        <table style="width: 100%" border="0" cellspacing="0" cellpadding="0">
        <tbody>
            <tr>
            <td valign="top" style="width: 40px">&nbsp;</td>
            <td>
            <table style="width: 100%" border="0" cellspacing="0" cellpadding="0">
            <tbody>
            <tr><td height="10px" class="spechide"></td></tr>
            <tr>
            <td class="movableContentContainer" valign="top">
            <div class="movableContent" style="margin: 0px; border: 0px; padding-top: 20px; position: relative;">
                <table style="width: 100%" border="0" cellspacing="0" cellpadding="0">
                <tbody>
                    <tr><td height="35"></td></tr>
                    <tr><td>
                    <table style="width: 100%" border="0" cellspacing="0" cellpadding="0">
                    <tbody>
                        <tr>
                        <td valign="top" align="center" class="specbundle">
                            <div class="contentEditableContainer contentTextEditable" style="margin: 0px;">
                                <div class="contentEditable" style="margin: 0px;">
                                    <p style="color: rgb(34, 34, 34); margin: 0px; font-size: 26px; line-height: 19px;">
                                        <span class="specbundle2">#localize("welcome.header", "locale", locale)</span>
                                    </p>
                                </div>
                            </div>
                        </td></tr>
                    </tbody>
                    </table>
                    </td></tr>
                </tbody>
                </table>
            </div>
            <div class="movableContent" style="margin: 0px; border: 0px; padding-top: 20px; position: relative;">
            <table style="width: 100%" border="0" cellspacing="0" cellpadding="0" align="center">
            <tbody>
                <tr><td align="left">
                    <div class="contentEditableContainer contentTextEditable" style="margin: 0px;">
                    <div class="contentEditable" align="center" style="margin: 0px;">
                    #import("header2")
                    </div>
                    </div>
                </td></tr>
                <tr><td height="10px"></td></tr>
                <tr><td align="left">
                    <div class="contentEditableContainer contentTextEditable" style="margin: 0px;">
                    <div class="contentEditable" align="left" style="margin: 0px;">
                        <p style=3D"color: rgb(119, 119, 119); margin: 0px; text-align: left; font-size: 14px; line-height: 19px;">
                        #if(recipientName == ""):
                            #localize("hello.recipientmail", "locale", locale, "recipientEmail", recipientEmail)
                        #else:
                            #localize("hello.recipient", "locale", locale, "recipientName", recipientName, "recipientEmail", recipientEmail)
                        #endif
                        <br>#import("emailbody")<br>
                        <br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                        <br>#localize("final.cheers", "locale", locale)<br>
                        <span style="color: rgb(34, 34, 34);">#localize("final.name", "locale", locale)</span>
                    </p>
                    </div>
                    </div>
                </td></tr>
                <tr><td height="35px"></td></tr>
                <tr><td align="center">
                <table>
                <tbody>
                <tr><td align="center" bgcolor="#3399cc" style="background: rgb(26, 84, 186); padding: 15px 18px; border-radius: 4px;">
                    <div class="contentEditableContainer contentTextEditable" style="margin: 0px;">
                    <div class="contentEditable" align="center" style="margin: 0px;">
                    #import("emailbutton")
                    </div>
                    </div>
                </td></tr>
                </tbody>
                </table>
                <br>
                <p style="color: rgb(119, 119, 119); margin: 0px; text-align: left; font-size: 14px; line-height: 19px;">#localize("footer.linkproblem", "locale", locale)</p>
                <p style="color: rgb(119, 119, 119); margin: 0px; text-align: left; font-size: 14px; line-height: 19px;">
                #import("emaillink")
                </p>
                #import("emailwarning")
            </td></tr>
            <tr><td height="20px"></td></tr>
        </tbody>
        </table>
        </div>
        <div class="movableContent" style="margin: 0px; border: 0px; padding-top: 20px; position: relative;">
        <table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tbody>
        <tr><td height="65px"></td></tr>
        <tr><td style="border-bottom-width: 1px; border-bottom-style: solid; border-bottom-color: rgb(221, 221, 221);"></td></tr>
        <tr><td height="25px"></td></tr>
        <tr><td>
        <table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tbody>
        <tr><td valign="top" class="specbundle">
            <div class="contentEditableContainer contentTextEditable" style="margin: 0px;">
            <div class="contentEditable" align="left" style="margin: 0px;">
            <p style="margin: 0 0 24px 0; padding: 0; color: #252525; font-family: Arial, Helvetica, &#39;sans-serif&#39;; font-weight: normal;  word-wrap: break-word; word-break: break-word; font-size: 12px; line-height: 16px; color: #909090;">
            #localize("final.notice", "locale", locale)<br>
            <a href="#localize("final.url", "locale", locale)" target="_blank" style="color: rgb(56, 47, 46); font-weight: bold;">#localize("final.urltext", "locale", locale)</a>
            </p>
            <div title="footer" style="margin: 0; padding: 40px 0 0 0; box-sizing: border-box;">
            <div title="copyright" style="margin: 0; padding: 0; font-size:10px; opacity:0.6; font-weight: 300; line-height: 12px; color: #252525; font-family: Arial, Helvetica, &#39;sans-serif&#39;; font-weight: normal;  word-wrap: break-word; word-break: break-word;">
            #localize("footer.copyright", "locale", locale)
            </div>
            </div>
            </div>
            </div>
        </td></tr>
        </tbody>
        </table>
    </td></tr>
    </tbody>
    </table>
</body>
</html>
